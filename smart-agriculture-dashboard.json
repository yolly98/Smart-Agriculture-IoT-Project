          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  v_type,\n  V.violations/M.measurements\nFROM (SELECT \n    CONCAT(\"land_\",CAST(land_id AS CHAR(100)), \"-\", sensor) AS v_type,\n    COUNT(*) AS violations \n  FROM violation \n  GROUP BY land_id, sensor\n  ) AS V INNER JOIN \n  (SELECT \n    CONCAT(\"land_\",CAST(land_id AS CHAR(100)), \"-\", sensor) AS m_type,\n    COUNT(*) AS measurements \n  FROM measurement \n  GROUP BY land_id, sensor\n  ) AS M\n  ON v_type=m_type\nORDER BY v_type",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "land_id"
                ],
                "type": "column"
              }
